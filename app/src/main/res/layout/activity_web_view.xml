<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:zhy="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.mosai.corporatetraining.activity.CategoryActivity">
    <RelativeLayout
        android:background="@color/titlecolor"
        android:layout_width="match_parent"
        android:layout_height="@dimen/titleheight"
        >

        <include layout="@layout/layout_ib_titile_back"></include>


        <include layout="@layout/layout_title_marqueetextview"/>

    </RelativeLayout>
    <include layout="@layout/layout_horizontalline"></include>
    <com.mosai.ui.HorizontalProgressBarWithNumber
        android:visibility="gone"
        android:max="100"
        android:id="@+id/hprogressbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        zhy:progress_reached_bar_height="2dp"
        zhy:progress_text_color="@color/colorPrimary"
        zhy:progress_unreached_color="@color/line_color_gray_light" />
    <WebView
        android:id="@+id/webview"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_width="match_parent"/>
</LinearLayout>
